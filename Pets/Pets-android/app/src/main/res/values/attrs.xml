<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="tab_theme" format="reference" />
    <!-- SohaButton -->
    <declare-styleable name="tab_layout">
        <attr name="tab_theme" />
    </declare-styleable>

    <declare-styleable name="CustomButtonBlack">
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomButtonBlue" >
        <attr name="value" format="string"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

</resources>